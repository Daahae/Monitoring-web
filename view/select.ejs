  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        현황판
        <small>2018</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> SW경시대회</a></li>
        <li><a href="#">현황판</a></li>
        <li class="active">2018</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="col">
        <div class="col-md-6">
          <div class="box">
            <div class="box-header with-border">
              <h3 class="box-title"><b>정답자</b></h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table class="table table-bordered">
                <tr>
                  <th style="width: 10px">#</th>
                  <th style="width: 70px">학번</th>
                  <th>이름</th>
                  <th>학과</th>
                  <th style="width: 40px">학년</th>
                </tr>
                <% for(var i = 0; i<score100Info.length; i++) { %>
                    <tr>
                        <td><%- i + 1 %></td>
                        <td><%- score100Info[i].ID %></td>
                        <td>
                          <%- score100Info[i].Name %>
                        </td>
                        <td>
                          <%- score100Info[i].Major %>
                        </td>
                        <td>
                          <% if(score100Info[i].Year == 1) { %>
                            <span class="badge bg-yellow">
                          <% } %>
                          <% if(score100Info[i].Year == 2) { %>
                            <span class="badge bg-green">
                          <% } %>
                          <% if(score100Info[i].Year == 3) { %>
                            <span class="badge bg-blue">
                          <% } %>
                          <% if(score100Info[i].Year == 4) { %>
                            <span class="badge bg-red">
                          <% } %>
                          <%- score100Info[i].Year %>학년
                        </span></td>
                      </tr>
                <% } %>

              </table>
            </div>
            <!-- /.box-body -->
            <div class="box-footer clearfix">
              <ul class="pagination pagination-sm no-margin pull-right">
                <li><a href="#">&laquo;</a></li>
                <li><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">&raquo;</a></li>
              </ul>
            </div>
          </div>
        </div>
      </div>
          <!-- /.box -->
  <!-------------------------------------------------------------------- -->
